{
  "name": "zz-frontend-boilerplate",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/zimmerman-zimmerman/zz-frontend-boilerplate.git"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.40",
    "@nivo/bar": "^0.59.2",
    "@nivo/circle-packing": "^0.61.0",
    "@storybook/csf": "^0.0.1",
    "@turf/helpers": "^6.1.4",
    "@types/react-map-gl": "^5.1.1",
    "auth0-js": "^9.11.3",
    "axios": "^0.19.2",
    "babel-plugin-macros": "^2.8.0",
    "clsx": "^1.0.4",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "dotenv": "^8.2.0",
    "easy-peasy": "^3.2.1",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.7.14",
    "mongoose-auto-increment": "^5.0.1",
    "mui-datatables": "^2.14.0",
    "multer": "^1.4.2",
    "nodemon": "^2.0.0",
    "paths.macro": "^2.0.2",
    "postmark": "2.2.4",
    "pretty-quick": "^2.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-fetching-library": "^1.5.12",
    "react-map-gl": "^5.2.2",
    "react-mapbox-gl-geocoder": "^1.1.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "react-use": "^13.8.0",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-encrypt": "^2.0.1",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "styled-components": "^5.0.0",
    "supercluster": "^7.0.0",
    "ts-node": "^8.5.2",
    "which-country": "^1.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.12",
    "@storybook/addon-links": "^5.3.12",
    "@storybook/addons": "^5.3.12",
    "@storybook/preset-create-react-app": "^1.5.2",
    "@storybook/react": "^5.3.12",
    "@types/auth0-js": "^9.10.6",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/helmet": "^0.0.45",
    "@types/jest": "24.0.22",
    "@types/lodash": "^4.14.149",
    "@types/material-ui": "^0.21.7",
    "@types/mui-datatables": "^2.13.4",
    "@types/node": "^12.12.11",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "@types/react-map-gl": "^5.1.1",
    "@types/react-router-dom": "^5.1.2",
    "@types/redux-persist": "^4.3.1",
    "@types/redux-persist-transform-encrypt": "^2.0.1",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "@types/storybook__react": "^5.2.1",
    "@types/styled-components": "^4.4.0",
    "babel-plugin-styled-components": "^1.10.6",
    "codecrumbs": "^1.7.3",
    "depcheck": "^0.9.0",
    "eslint-config-airbnb-typescript-prettier": "^1.4.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^4.2.1",
    "prettier": "^1.18.2",
    "source-map-explorer": "^2.1.0",
    "storybook-addon-playroom": "^1.0.0",
    "storybook-chroma": "^3.4.0",
    "typescript": "^3.7.2"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "depcheck": "depcheck",
    "codecrumbs": "codecrumbs -e src/index.tsx -d src -p 1234 ",
    "start-back": "nodemon --watch 'server/' --exec ts-node --project 'nodetsconfig.json' server/server.ts",
    "start-fullstack": "yarn start & yarn start-back",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public --ci",
    "build-storybook": "build-storybook -s public",
    "init-data": "ts-node --project 'nodetsconfig.json' server/scripts/loadInitialData.ts"
  },
  "babelMacros": {
    "styledComponents": {
      "pure": true
    }
  },
  "eslintConfig": {
    "settings": {
      "import/resolver": {
        "node": {
          "paths": [
            "src"
          ]
        }
      }
    },
    "parserOptions": {
      "project": "./tsconfig.json",
      "tsconfigRootDir": "./"
    },
    "extends": [
      "react-app",
      "airbnb-typescript-prettier"
    ],
    "rules": {
      "import/prefer-default-export": 0,
      "react-hooks/exhaustive-deps": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/ban-ts-ignore ": 0,
      "@typescript-eslint/no-unused-vars": 0
    },
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "no-shadow": "off",
          "no-unused-vars": "off",
          "react/destructuring-assignment": 0,
          "react/jsx-fragments": 0,
          "react/jsx-props-no-spreading": 0,
          "react-hooks/exhaustive-deps": 0,
          "@typescript-eslint/no-explicit-any": 0,
          "@typescript-eslint/ban-ts-ignore ": 0,
          "@typescript-eslint/no-unused-vars": 0,
          "@typescript-eslint/no-empty-function": 0,
          "no-underscore-dangle": "off",
          "no-useless-escape": "off"
        }
      }
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "macros"
      ],
      [
        "babel-plugin-styled-components",
        {
          "ssr": false
        }
      ]
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "proxy": "http://localhost:4200"
}

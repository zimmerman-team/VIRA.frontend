    "easy-peasy": "^3.3.0",
    "history": "^4.10.1",
    "i18next": "^19.3.3",
    "i18next-browser-languagedetector": "^4.0.2",
    "lodash": "^4.17.15",
    "mui-datatables": "^2.14.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-fetching-library": "^1.7.0",
    "react-hot-loader": "^4.12.21",
    "react-i18next": "^11.3.4",
    "react-map-gl": "^5.2.3",
    "react-mapbox-gl-geocoder": "^1.1.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.1",
    "react-use": "^13.27.0",
    "redux-persist": "^6.0.0",

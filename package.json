{
  "name": "zz-frontend-boilerplate",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/zimmerman-zimmerman/zz-frontend-boilerplate.git"
  },
  "dependencies": {
    "@aws-amplify/api": "latest",
    "@aws-amplify/pubsub": "latest",
    "@material-ui/core": "^4.6.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.31",
    "@nivo/axes": "^0.59.2",
    "@nivo/bar": "^0.59.2",
    "auth0-js": "^9.11.3",
    "cors": "^2.8.5",
    "csstype": "^2.6.7",
    "dotenv": "^8.2.0",
    "easy-peasy": "^3.2.1",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "lodash": "^4.17.15",
    "mui-datatables": "^2.12.4",
    "nodemon": "^2.0.0",
    "postmark": "2.2.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-fetching-library": "^1.5.12",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0-next.62",
    "react-use": "^13.8.0",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-encrypt": "^2.0.1",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "styled-components": "^5.0.0-rc.2",
    "swr": "^0.1.10",
    "ts-node": "^8.5.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-links": "^5.2.5",
    "@storybook/addons": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "@types/auth0-js": "^9.10.6",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/helmet": "^0.0.45",
    "@types/jest": "24.0.22",
    "@types/lodash": "^4.14.149",
    "@types/material-ui": "^0.21.7",
    "@types/mui-datatables": "^2.12.2",
    "@types/node": "^12.12.11",
    "@types/react": "^16.9.13",
    "@types/react-dom": "16.9.4",
    "@types/react-router-dom": "^5.1.2",
    "@types/redux-persist": "^4.3.1",
    "@types/redux-persist-transform-encrypt": "^2.0.1",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "^4.4.0",
    "babel-plugin-styled-components": "^1.10.6",
    "codecrumbs": "^1.7.3",
    "cypress": "^3.6.1",
    "depcheck": "^0.9.0",
    "eslint-config-airbnb-typescript-prettier": "^1.4.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.1.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.1",
    "source-map-explorer": "^2.1.0",
    "storybook-chroma": "^3.4.0",
    "stylelint": "^12.0.0",
    "stylelint-prettier": "^1.1.1",
    "typescript": "^3.7.2"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "depcheck": "depcheck",
    "codecrumbs": "codecrumbs -e src/index.tsx -d src -p 1234 ",
    "start-front": "react-scripts start",
    "start-back": "nodemon --watch 'server/' --exec ts-node --project 'nodetsconfig.json' server/server.ts",
    "start": "yarn start-front & yarn start-back",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "settings": {
      "import/resolver": {
        "node": {
          "paths": [
            "src"
          ]
        }
      }
    },
    "parserOptions": {
      "project": "./tsconfig.json",
      "tsconfigRootDir": "./"
    },
    "extends": [
      "react-app",
      "airbnb-typescript-prettier"
    ],
    "rules": {
      "import/prefer-default-export": 0
    },
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "no-unused-vars": "off",
          "react/destructuring-assignment": 0,
          "react/jsx-fragments": 0,
          "react/jsx-props-no-spreading": 0
        }
      }
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "babel-plugin-styled-components",
        {
          "ssr": false
        }
      ]
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "proxy": "http://localhost:4200"
}

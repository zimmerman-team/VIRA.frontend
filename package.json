{
  "name": "zz-frontend-boilerplate",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/zimmerman-zimmerman/zz-frontend-boilerplate.git"
  },
  "dependencies": {
    "@material-ui/core": "^4.6.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.31",
    "csstype": "^2.6.7",
    "easy-peasy": "^3.2.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0-next.39",
    "react-use": "^13.3.0",
    "styled-components": "^5.0.0-rc.1",
    "swr": "^0.1.8"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-links": "^5.2.5",
    "@storybook/addons": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "@types/jest": "24.0.22",
    "@types/material-ui": "^0.21.7",
    "@types/node": "12.12.6",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.4",
    "@types/react-router-dom": "^5.1.2",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "^4.1.22",
    "babel-plugin-styled-components": "^1.10.6",
    "codecrumbs": "^1.7.3",
    "cypress": "^3.6.0",
    "depcheck": "^0.9.0",
    "eslint-config-airbnb-typescript-prettier": "^1.4.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.1",
    "source-map-explorer": "^2.1.0",
    "stylelint": "^11.1.1",
    "stylelint-prettier": "^1.1.1",
    "typescript": "3.7.2"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "depcheck": "depcheck",
    "codecrumbs": "codecrumbs -e src/index.tsx -d src -p 1234 ",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "settings": {
      "import/resolver": {
        "node": {
          "paths": [
            "src"
          ]
        }
      }
    },
    "parserOptions": {
      "project": "./tsconfig.json",
      "tsconfigRootDir": "./"
    },
    "extends": [
      "react-app",
      "airbnb-typescript-prettier"
    ],
    "rules": {
      "import/prefer-default-export": 0
    },
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "no-unused-vars": "off",
          "react/destructuring-assignment": 0
        }
      }
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "babel-plugin-styled-components",
        {
          "ssr": false
        }
      ]
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}

    "depcheck": "depcheck",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "cypress:open": "export BABEL_ENV=development && cypress open",
    "cypress:record": "export BABEL_ENV=development && cypress run --record --key 30a16df3-0b07-4a7e-bd9c-d6a60c8c8354",
    "cypress:record:chrome": "export BABEL_ENV=development && cypress run --browser chrome --record --key 30a16df3-0b07-4a7e-bd9c-d6a60c8c8354",
    "cypress:record:canary": "export BABEL_ENV=development && cypress run --browser chrome:canary --record --key 30a16df3-0b07-4a7e-bd9c-d6a60c8c8354",
    "happo:run": "happo run [sha]",
    "happo-ci-circleci": "BASE_BRANCH=\"origin/develop\" happo-ci-circleci"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "release": {
    "branches": [
      "master",
      {
        "name": "develop",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  }

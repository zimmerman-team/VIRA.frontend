    "source-map-explorer": "^2.3.1",
    "storybook-addon-deps": "^1.0.12",
    "storybook-addon-playroom": "^1.0.0",
    "storybook-chromatic": "^3.5.2",
    "storybook-vrhythm": "^0.0.4",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.2"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "depcheck": "depcheck",
    "codecrumbs": "codecrumbs -e src/index.tsx -d src -p 1234 ",
    "start-back": "nodemon --watch 'server/' --exec ts-node --project 'nodetsconfig.json' server/server.ts",
    "start-fullstack": "yarn start & yarn start-back",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public --ci process.env.DEV_ENVIRONMENT=isolated",
    "build-storybook": "build-storybook -s public",
    "init-data": "ts-node --project 'nodetsconfig.json' server/scripts/loadInitialData.ts",
    "chromatic": "chromatic",
    "cypress:open": "cypress open",
    "cypress:record": "cypress run --record --key 30a16df3-0b07-4a7e-bd9c-d6a60c8c8354"
  },
  "babelMacros": {
    "styledComponents": {
      "pure": true
    }
  },
  "eslintConfig": {
    "settings": {
      "import/resolver": {
        "node": {
          "paths": [
            "src"
          ]
        }
      }
    },
    "parserOptions": {
      "project": "./tsconfig.json",
      "tsconfigRootDir": "./"
    },
    "extends": [
      "react-app",
      "airbnb-typescript-prettier"
    ],
    "rules": {
      "import/prefer-default-export": 0,
      "react-hooks/exhaustive-deps": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/ban-ts-ignore ": 0,
      "@typescript-eslint/no-unused-vars": 0
    },
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "no-shadow": "off",
          "no-unused-vars": "off",
          "react/destructuring-assignment": 0,
          "react/jsx-fragments": 0,
          "react/jsx-props-no-spreading": 0,
          "react-hooks/exhaustive-deps": 0,
          "@typescript-eslint/no-explicit-any": 0,
          "@typescript-eslint/ban-ts-ignore ": 0,
          "@typescript-eslint/no-unused-vars": 0,
          "@typescript-eslint/no-empty-function": 0,
          "no-underscore-dangle": "off",
          "no-useless-escape": "off"
        }
      }
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "babel-plugin-styled-components",
        {
          "ssr": false
        }
      ]
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "proxy": "http://localhost:4200"
}

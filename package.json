  "devDependencies": {
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-docs": "^5.3.14",
    "@storybook/addon-links": "^5.3.14",
    "@storybook/addon-viewport": "^5.3.14",
    "@storybook/addons": "^5.3.12",
    "@storybook/csf": "^0.0.1",
    "@storybook/preset-create-react-app": "^2.0.0",
    "@storybook/preset-typescript": "^2.1.0",
    "@storybook/react": "^5.3.14",
    "@testing-library/cypress": "^6.0.0",
    "@testing-library/dom": "^7.2.1",
    "@testing-library/react": "^10.0.2",
    "@types/auth0-js": "^9.10.6",
    "@types/i18next-xhr-backend": "^1.4.2",
    "@types/jest": "24.0.22",
    "@types/lodash": "^4.14.149",
    "@types/material-ui": "^0.21.7",
    "@types/mui-datatables": "^2.13.4",
    "@types/node": "^12.12.11",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-map-gl": "^5.2.0",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-logger": "^3.0.7",
    "@types/redux-persist": "^4.3.1",
    "@types/redux-persist-transform-encrypt": "^2.0.1",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "@types/styled-components": "^5.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-react-data-testid": "^0.1.1",
    "babel-plugin-styled-components": "^1.10.7",
    "cypress": "^4.5.0",
    "cypress-shadow-dom": "^1.3.0",
    "depcheck": "^0.9.2",
    "dotenv-extended": "^2.8.0",
    "eslint-config-airbnb-typescript-prettier": "^1.4.2",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "fork-ts-checker-webpack-plugin": "^4.0.5",
    "getenv": "^1.0.0",
    "husky": "^4.2.3",
    "jest": "24.9.0",
    "paths.macro": "^2.0.2",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-is": "^16.13.0",
    "source-map-explorer": "^2.3.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^3.8.2",
    "typescript-plugin-styled-components": "^1.4.4",
    "webpack-plugin-ramdisk": "^0.1.4"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "depcheck": "depcheck",
    "codecrumbs": "codecrumbs -e src/index.tsx -d src -p 1234 ",
    "start-back": "nodemon --watch 'server/' --ignore 'server/__test__' --exec ts-node --project 'nodetsconfig.json' server/server.ts",
    "start-fullstack": "yarn start & yarn start-back",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "test-api": "jest --detectOpenHandles --runTestsByPath ./server/__test__/*.spec.ts",
    "start-back-test": "ts-node --project 'nodetsconfig.json' server/server",
    "start-test-api": "start-server-and-test start-back-test http://localhost:4200/api test-api",
    "storybook": "start-storybook -p 9009 -s public --ci process.env.DEV_ENVIRONMENT=isolated",
    "build-storybook": "build-storybook -s public",
    "init-data": "ts-node --project 'nodetsconfig.json' server/scripts/loadInitialData.ts",
    "cypress:open": "cypress open",
    "cypress:record": "cypress run --record --key 30a16df3-0b07-4a7e-bd9c-d6a60c8c8354"
  },
  "eslintConfig": {
    "settings": {
      "import/resolver": {
        "node": {
          "paths": [
            "src"
          ]
        }
      }
    },
    "parserOptions": {
      "project": "./tsconfig.json",
      "tsconfigRootDir": "./"
    },
    "extends": [
      "react-app",
      "airbnb-typescript-prettier"
    ],
    "rules": {
      "import/prefer-default-export": 0,
      "react-hooks/exhaustive-deps": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/ban-ts-ignore ": 0,
      "@typescript-eslint/no-unused-vars": 0,
      "no-restricted-imports": [
        "error",
        {
          "paths": [
            {
              "name": "styled-components",
              "message": "Please import from styled-components/macro."
            }
          ],
          "patterns": [
            "!styled-components/macro"
          ]
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "no-shadow": "off",
          "no-unused-vars": "off",
          "react/destructuring-assignment": 0,
          "react/jsx-fragments": 0,
          "react/jsx-props-no-spreading": 0,
          "react-hooks/exhaustive-deps": 0,
          "@typescript-eslint/no-explicit-any": 0,
          "@typescript-eslint/ban-ts-ignore ": 0,
          "@typescript-eslint/no-unused-vars": 0,
          "@typescript-eslint/no-empty-function": 0,
          "no-underscore-dangle": "off",
          "no-useless-escape": "off"
        }
      }
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "release": {
    "branches": [
      "master"
    ]
  },
  "proxy": "http://localhost:4200"
}
